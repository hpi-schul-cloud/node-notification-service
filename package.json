	},
	"license": "MIT",
	"author": "",
	"main": "src/app",
	"types": "src/app",
	"scripts": {
		"build": "tsc --project tsconfig.production.json",
		"build:all": "tsc",
		"debug": "concurrently \"tsc -w\" \"nodemon\"",
		"lint": "eslint --fix --ignore-path .gitignore .",
		"start": "node -r ts-node/register -r tsconfig-paths/register build/src/app.js",
		"test": "mocha -t 30000 -r tsconfig-paths/register -r ts-node/register test/**/*.spec.ts --exit",
		"clean": "rm -rf build node_modules",
		"clean:build": "rm -rf build"
	},
