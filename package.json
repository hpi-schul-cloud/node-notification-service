{
  "name": "node-notification-service",
  "description": "feathers app to provide a push notification api",
  "version": "0.0.0",
  "homepage": "https://github.com/schulcloud/node-notification-service",
  "main": "src/",
  "keywords": [
    "push",
    "notifications"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/schulcloud/node-notification-service.git"
  },
  "bugs": {
    "url": "https://github.com/schulcloud/node-notification-service/issues"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "start": "nodemon --inspect src/",
    "test": "npm run coverage && npm run eslint",
    "testing": "nodemon --inspect src/ --exec \"npm test  || true\"",
    "coverage": "cross-env NODE_ENV=testing ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha ./test/**/*.test.js",
    "eslint": "eslint src/. test/. ",
    "jshint": "jshint src/. test/. --config"
  },
  "dependencies": {
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.0",
    "@feathersjs/feathers": "^3.1.3",
    "apn": "^2.1.2",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "feathers-mongoose": "^3.6.1",
    "feathers-swagger": "^0.3.1",
    "jsonapi-serializer": "^3.4.2",
    "mongoose": "4.7.8",
    "node-gcm": "^0.14.4",
    "serve-favicon": "^2.3.0",
    "winston": "^2.3.0",
    "yarn": "^1.5.1"
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "eslint": "^2.13.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.4",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "request": "^2.78.0",
    "request-promise": "^4.1.1",
    "sinon": "^1.17.6"
  }
}
