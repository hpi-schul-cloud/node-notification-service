     "description": "Get all Notifications",
     "parameters": [{
       "name": "user",
       "in": "query",
       "type": "string",
       "description": "Search for user id",
       "required": false
     },{
       "name": "state",
       "in": "query",
       "type": "string",
       "description": "Search for state",
       "required": false
     },{
       "name": "$limit",
       "in": "query",
       "type" : "integer",
       "description": "Maximum number of results",
       "required": false
     },{
       "name": "$skip",
       "in": "query",
       "type": "integer",
       "description": "Number skipped of results",
       "required": false
     },{
