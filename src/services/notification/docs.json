     "responses": {
       "200": {
         "description": "OK"
       },
       "404": {
         "description": "No Notifications found"
       },
       "500": {
         "description": "internal error try again later."
       }
     }
   },
   "get": {
     "summary": "Find a Notification by ID",
     "description": "Find Notification by ID",
     "responses": {
       "200": {
         "description": "OK"
       },
       "404": {
